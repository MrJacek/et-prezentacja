[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/nicecode (MrJacek)
[access "refs/for/refs/*"]
	read = group user/nicecode (MrJacek)
	push = group user/nicecode (MrJacek)
	label-Code-Review = -2..+2 group user/nicecode (MrJacek)
	label-Verified = -1..+1 group user/nicecode (MrJacek)
	rebase = group user/nicecode (MrJacek)
	removeReviewer = group user/nicecode (MrJacek)
	submit = group user/nicecode (MrJacek)
[access "refs/heads/*"]
	read = group user/nicecode (MrJacek)
	create = group user/nicecode (MrJacek)
	push = +force group user/nicecode (MrJacek)
	pushMerge = group user/nicecode (MrJacek)
[access "refs/tags/*"]
	push = group user/nicecode (MrJacek)
	push = +force group user/nicecode (MrJacek)
